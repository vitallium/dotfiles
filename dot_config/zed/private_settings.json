{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-20240620-fast-edit"
    },
    "version": "2"
  },
  "slash_commands": {
    "docs": {
      "enabled": true
    }
  },
  "theme": {
    "light": "One Light",
    "dark": "One Dark"
  },
  "buffer_font_family": "Berkeley Mono Variable",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "journal": {
    "hour_format": "hour24",
    "path": "~/Documents/Journal"
  },
  "languages": {
    "Ruby": {
      "language_servers": ["ruby-lsp", "rubocop", "!solargraph"]
    }
  },
  "features": {
    "inline_completion_provider": "none"
  },
  "lsp": {
    "rust-analyzer": {
      "inlayHints": {
        "maxLength": null,
        "lifetimeElisionHints": {
          "useParameterNames": true,
          "enable": "skip_trivial"
        },
        "closureReturnTypeHints": {
          "enable": "always"
        }
      }
    },
    "solargraph": {
      "initialization_options": {
        "diagnostics": false,
        "formatting": false
      }
    },
    "ruby-lsp": {
      "initialization_options": {
        "experimentalFeaturesEnabled": true,
        "featuresConfiguration": {
          "inlayHint": {
            "enableAll": true
          }
        }
      }
    },
    "rubocop": {
      "initialization_options": {
        "safeAutocorrect": false
      }
    }
  }
}
